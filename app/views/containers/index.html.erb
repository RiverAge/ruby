<% provide(:title, "All Container") %>

<h1>CONTAINER</h1>
<div class="table-responsive">
  <table class="dataTable table table-hover" id="containerlist">
    <thead>
    <tr>
      <th>id</th>
      <th>状态</th>
      <th>提单号</th>
      <th>箱数</th>
      <th>负责人</th>
      <th>入库提交时间</th>
      <th>出库状态</th>
    </tr>
    </thead>

    <tbody>
    <% @containers.each do |container| %>
        <tr>
          <td><%= container.id %> </td>
          <td><%= container.状态 %> </td>
          <td><%= container.提单号 %> </td>
          <td><%= container.箱数 %> </td>
          <td><%= container.负责人 %> </td>
          <td><%= container.入库提交时间 %> </td>
          <td><%= container.出库状态 %> </td>
        </tr>
    <% end %>
    </tbody>

  </table>

</div>