<% provide(:title, "All Users") %>

  <%= paginate @users%>
<div class="table-responsive">
  <table class="table table-hover">
    <% @users.each do |user| %>
        <tr>
          <td><%= link_to user.name, user %></td>
          <td><%= link_to user.email, user %></td>
        </tr>
    <% end %>
  </table>

</div>
  <%= paginate @users%>

